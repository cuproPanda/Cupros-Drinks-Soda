<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="TreeBase">
		<defName>CPD_PlantFrassRoot</defName>
		<label>frassroot</label>
		<description>A long, tangly root plant that occasionally pokes above the surface of the ground. Quite tasty once brewed.</description>
		<statBases>
			<MaxHitPoints>85</MaxHitPoints>
			<Beauty>10</Beauty>
		</statBases>
		<graphicData>
			<texPath>Cupro/Object/Plant/FrassRoot</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<shadowData>
				<volume>(0,0,0)</volume>
			</shadowData>
		</graphicData>
		<fillPercent>0.01</fillPercent>
		<altitudeLayer>LowPlant</altitudeLayer>
		<selectable>true</selectable>
		<plant>
			<dieIfLeafless>false</dieIfLeafless>
			<harvestTag>Standard</harvestTag>
			<harvestedThingDef>CPD_FrassRoot</harvestedThingDef>
			<harvestYield>3</harvestYield>
			<harvestWork>500</harvestWork>
			<harvestAfterGrowth>0.20</harvestAfterGrowth>
			<blockAdjacentSow>false</blockAdjacentSow>
			<topWindExposure>0.1</topWindExposure>
			<growDays>10</growDays>
			<lifespanFraction>4</lifespanFraction>
			<fertilitySensitivity>0.8</fertilitySensitivity>
			<wildCommonalityMaxFraction>0.5</wildCommonalityMaxFraction>
			<wildBiomes>
				<TropicalRainforest>0.02</TropicalRainforest>
				<TemperateForest>0.02</TemperateForest>
				<AridShrubland>0.01</AridShrubland>
				<BorealForest>0.02</BorealForest>
				<Desert>0.01</Desert>
				<ExtremeDesert>0.01</ExtremeDesert>
				<Tundra>0.01</Tundra>
			</wildBiomes>
			<visualSizeRange>
				<min>0.1</min>
				<max>1.1</max>
			</visualSizeRange>
			<wildClusterSizeRange>
				<min>3</min>
				<max>10</max>
			</wildClusterSizeRange>
			<wildClusterRadius>8</wildClusterRadius>
		</plant>
	</ThingDef>

</Defs>